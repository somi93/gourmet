{% extends 'base.html.twig' %}

{% block content %}

<div class="content">
    <div class="container_12">
        <div class="grid_6">
            <h2>Find Us</h2>
            <div class="map">
                <figure class="img_inner">
                    <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3366.1979009051706!2d20.477829578915898!3d44.80885600875382!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x475a7aa3d7b53fbd%3A0x1db8645cf2177ee4!2z0JHQtdC-0LPRgNCw0LQ!5e0!3m2!1ssr!2srs!4v1485876016137" width="600" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
                </figure>
                <address>
                    <dl>
                        <dt>
                        <p>The Gourmet<br>
                            210 Bulevar Mihajla Pupina,<br>
                            Belgrade, 11000 Serbia.</p>
                        </dt>
                        <dd><span>Freephone:</span>+1 800 559 6580</dd>
                        <dd><span>Telephone:</span>+1 800 603 6035</dd>
                        <dd><span>FAX:</span>+1 800 889 9898</dd>
                    </dl>
                </address>
            </div>
        </div>
        <div class="grid_5 prefix_1">
            <h2>Contact Us</h2>
            <form id="form" action="#">
                <div class="success_wrapper"></div>
                <fieldset>
                    <label class="name">
                        <input type="text" placeholder="Name" class="form-input">
                    </label>
                    <br/>
                    <label class="email">
                        <input type="text" placeholder="E-mail" class="form-input">
                    </label>
                    <br/>
                    <label class="phone">
                        <input type="tel" placeholder="Phone" class="form-input">
                    </label>
                    <br/>
                    <label class="message">
                        <textarea class="form-input" placeholder="Message"></textarea>
                    </label>
                    <div class="clear"></div>
                    <div class="btns">
                        <a data-type="reset" class="btn">clear</a>
                        <a data-type="submit" class="btn">send</a>
                        <div class="clear"></div>
                    </div>
                </fieldset>
            </form>
        </div>

{% endblock %}