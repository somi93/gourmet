{% extends 'base.html.twig' %}

{% block content %}
    <div class="content">
        <div class="container_12">
            <div class="grid_12">
                <h2>Portfolio</h2>
            </div>
            <div class="clear"></div>
            <div class="portfolio">
                {% for document in documents %}
                    <div class="grid_6">
                        <a href="{{ asset(document.getBrochure()) }}" data-gal="prettyPhoto[1]">
                            <span></span><img src="{{ asset(document.getBrochure()) }}" alt="">
                        </a>
                    </div>
                {% endfor %}
            </div>
            <div class="clear"></div>
{% endblock %}